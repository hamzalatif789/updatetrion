{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(.*)\\.(js|css|png|jpg|svg|woff2?|ttf|map)$", "headers": { "cache-control": "public,max-age=31536000,immutable" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
